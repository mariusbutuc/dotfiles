[alias]
	br = branch
	co = checkout
	cp = cherry-pick
	st = status
[color]
        branch = auto
        diff = auto
        status = auto
        interactive = auto
        ui = true
[color "branch"]
        current = yellow reverse
        local = yellow
        remote = green
[color "diff"]
        meta = yellow bold
        frag = magenta bold
        old = red bold
        new = green bold
[color "status"]
        added = yellow
        changed = green
        untracked = cyan
[core]
        editor = vim
        excludesfile = ~/.gitignore_global
        whitespace = fix,-indent-with-non-tab,trailing-space,cr-at-eol
[github]
        user = mariusbutuc
[pull]
        rebase = true
[push]
        default = current
[rebase]
        autosquash = true
        autostash = true
[user]
        name = Marius Butuc
        email = marius.butuc@gmail.com
